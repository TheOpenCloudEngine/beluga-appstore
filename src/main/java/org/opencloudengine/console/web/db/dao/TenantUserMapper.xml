<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="org.opencloudengine.bss.sample.dao.TenantUserMapper" >
  <select id="selectAll" resultType="org.opencloudengine.bss.sample.entity.TenantUser">
  	select * from tenantUser
  </select>
  
  <select id="select" parameterType="map" resultType="org.opencloudengine.bss.sample.entity.TenantUser">
  	select * from tenantUser
  	where tenantId = #{tenantId} and userId=#{userId}
  </select>

  <insert id="insert" parameterType="org.opencloudengine.bss.sample.entity.TenantUser">
        INSERT INTO tenantUser(
            tenantId, userId
        ) VALUES(
            #{tenantId}, #{userId}
        )
    </insert>
    <delete id="truncate" >
        truncate table tenantUser
    </delete>
</mapper>